<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="CNN Client on Chrome Browser">

    <!--the first 3 parameters here are designed in case I want to run my framework through the cloud-->
    <parameter name="useCloudEnv" value="false"/>
    <parameter name="userName" value="alexei1"/>
    <parameter name="accessKey" value="6f132866-de72-43fd-b49a-e6f81d26bfe3"/>
    <parameter name="os" value="Windows"/>
    <parameter name="url" value="http://www.cnn.com/"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="50"/>

    <test name = "Cnn Home Page">
        <classes>
            <class name="home.LogIn"/>
            <class name="home.LogOut"/>
        </classes>
    </test>

</suite>
